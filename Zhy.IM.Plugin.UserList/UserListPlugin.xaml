<UserControl x:Class="Zhy.IM.Plugin.UserList.UserListPlugin"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <StackPanel>
        <Expander Name="expFriend" Header="我的好友">
            <StackPanel Orientation="Horizontal"
                        MouseLeftButtonDown="StackPanel_MouseLeftButtonDown_1">
                <Rectangle Width="45" Height="45" Fill="Blue"></Rectangle>
                <TextBlock Height="23" Text="Jack" Margin="5"></TextBlock>
                
            </StackPanel>
        </Expander>
        <Expander Name="expBlacklist" Header="黑名单"></Expander>
    </StackPanel>
</UserControl>
